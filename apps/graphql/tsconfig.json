{
  "compilerOptions": {
    "target": "es2017",
    "baseUrl": ".",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "isolatedModules": true,
    "paths": {
      "@gqlgen": [
        "generated/graphql"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}
